import request from '@/utils/request'
import qs from "qs";

// 查询船东列表
export function querySkipperInfo() {
  return request({
    url: '/transport/skipperManage/querySkipperInfo',
    method: 'get'
  })
}


// 条件查询船东列表
export function querySkippers(data) {
  return request({
    url: '/transport/skipperManage/querySkippers',
    method: 'post',
    data: data
  })
}


