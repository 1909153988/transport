userId) {\n  return request({\n    url: \"/client/clientAccountUser/roleMenuTreeselect/\" + userId,\n    method: \"get\"\n  });\n}\n// 修改用户菜单权限信息\nexport function updateUserMenuID(data) {\n  return request({\n    url: \"/client/clientAccountUser/editUserMenuId\",\n    method: \"put\",\n    data: data\n  });\n}\n"]}]}