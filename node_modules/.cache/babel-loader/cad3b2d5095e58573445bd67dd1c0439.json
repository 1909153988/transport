onents:{\n    ImageUpload\n  },\n  methods:{\n    submitForm(formName) {\n      this.$refs[formName].validate((valid) => {\n        if (valid) {\n          alert('submit!');\n        } else {\n          console.log('error submit!!');\n          return false;\n        }\n      });\n    }\n  }\n}\n</script>\n\n<style scoped>\n.el-divider--vertical {\n  width: 5px;\n  height: 1.5em;\n  background-color: #1890ff;\n}\n</style>\n"],"sourceRoot":"src/views/customercenter/consultation/add"}]}